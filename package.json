{
  "private": true,
  "name": "url-shortener",
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "remix build && run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix watch",
    "generate:css": "npx tailwindcss -i ./styles/app.css -o ./app/styles/app.css",
    "dev:wrangler": "cross-env NODE_ENV=development wrangler pages dev ./public --kv LONG_URL_KV",
    "start": "cross-env NODE_ENV=production npm run dev:wrangler"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^1.5.1",
    "@remix-run/cloudflare-pages": "^1.5.1",
    "@remix-run/react": "^1.5.1",
    "@splitbee/web": "^0.3.0",
    "cross-env": "^7.0.3",
    "nanoid": "^4.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.11.0",
    "@remix-run/dev": "^1.5.1",
    "@remix-run/eslint-config": "^1.5.1",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "esbuild": "0.14.43",
    "eslint": "^8.17.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.1.2",
    "typescript": "^4.7.3",
    "wrangler": "beta"
  },
  "engines": {
    "node": ">=14"
  }
}
