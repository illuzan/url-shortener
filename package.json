{
  "private": true,
  "name": "url-shortener",
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "remix build && run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix watch",
    "generate:css": "npx tailwindcss -i ./styles/app.css -o ./app/styles/app.css",
    "dev:wrangler": "cross-env NODE_ENV=development wrangler pages dev ./public --kv LONG_URL_KV",
    "start": "cross-env NODE_ENV=production npm run dev:wrangler"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^1.15.0",
    "@remix-run/cloudflare-pages": "^1.15.0",
    "@remix-run/react": "^1.15.0",
    "@splitbee/web": "^0.3.0",
    "cross-env": "^7.0.3",
    "isbot": "^3.6.8",
    "nanoid": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.19.0",
    "@remix-run/dev": "^1.15.0",
    "@remix-run/eslint-config": "^1.15.0",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "esbuild": "0.17.18",
    "eslint": "^8.38.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.3.1",
    "typescript": "^4.9.5",
    "wrangler": "^2.15.1"
  },
  "engines": {
    "node": ">=16.13"
  }
}